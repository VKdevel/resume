       <div class="row">
       <div class="col-12">
            {{#with data}}

            <div class="card text-bg-success text-center my-5">
                <div class="card-body">
                    <h5 class="card-title">{{title}}</h5>
                    <p class="ps-2">{{description}}</p>
                    {{#each buttons}}

                    <a class="btn btn-primary" href="{{link}}">{{text}}</a>
                    {{/each}}
                </div>
            </div>
            {{/with}}
        </div>
        </div>